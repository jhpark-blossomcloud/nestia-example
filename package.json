{
  "name": "picktogram-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "postbuild": "npx nestia swagger",
    "docker-compose": "docker-compose up",
    "docker-compose:build": "docker-compose -f docker-compose.local.yml up --build",
    "docker:build": "docker build -t picktogram/server:latest ./",
    "docker:start:local": "docker run -p 3000:3000 --env-file=.env picktogram/server:latest",
    "build": "nest build",
    "build:tsc": "tsc -p tsconfig.build.json",
    "start:local": "cross-env NODE_ENV=LOCAL nest start --watch",
    "start:dev": "cross-env NODE_ENV=DEVELOPMENT nest start --watch",
    "start:prod": "node dist/main",
    "start:pm2": "cross-env NODE_ENV=PRODUCTION pm2 start",
    "pm2:reload": "npm run build && pm2 reload main",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=TEST jest --watch --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "schema:drop:local": "cross-env NODE_ENV=LOCAL ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -d ./data-source.ts",
    "schema:sync:local": "cross-env NODE_ENV=LOCAL ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./data-source.ts",
    "schema:sync:dev": "cross-env NODE_ENV=DEVELOPMENT ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./data-source.ts",
    "schema:sync:test": "cross-env NODE_ENV=TEST ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./data-source.ts",
    "schema:drop:test": "cross-env NODE_ENV=TEST ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -d ./data-source.ts",
    "prepare": "ts-patch install && ts-patch install && ts-patch install && ts-patch install && ts-patch install && ts-patch install && ts-patch install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.276.0",
    "@nestia/core": "^1.0.12",
    "@nestia/fetcher": "^1.0.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1321.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "multer": "^1.4.3",
    "multer-s3": "^3.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.6.0",
    "swagger-ui-express": "^4.6.1",
    "typeorm": "^0.3.12",
    "typia": "^3.5.7"
  },
  "devDependencies": {
    "@nestia/sdk": "^1.0.8",
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.14.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-kakao": "^1.0.0",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "inquirer": "^9.1.4",
    "jest": "^29.4.3",
    "nestia": "^4.0.7",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "tsconfig-paths": "^4.1.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5"
  },
  "types": "src/types/index.d.ts",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
